set nocompatible
syntax on
set number
set ruler
set autoindent
set hlsearch
set expandtab
set tabstop=4
set shiftwidth=4
set softtabstop=4
set backspace=indent,eol,start
set vb t_vb=""
set mouse=a
filetype plugin indent on

set t_Co=256
