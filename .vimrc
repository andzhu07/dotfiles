set nocompatible
syntax on
set number
set ruler
set autoindent
set hlsearch
set expandtab
set tabstop=2
set shiftwidth=2
set softtabstop=2
set backspace=indent,eol,start
set clipboard=unnamed
set vb t_vb=""
filetype plugin indent on

set t_Co=256
color peachpuff
hi Comment ctermfg=green

