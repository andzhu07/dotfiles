syntax on
set ruler
set number
set hlsearch
set autoindent
set expandtab
set tabstop=4
set shiftwidth=4
set softtabstop=4
set backspace=indent,eol,start
filetype plugin indent on

colorscheme peachpuff
highlight Comment ctermfg=green

